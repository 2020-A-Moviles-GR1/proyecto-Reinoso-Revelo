<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".DetallesCitaActivity">

    <TextView
        android:id="@+id/textView34"
        android:layout_width="199dp"
        android:layout_height="43dp"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/comfortaa_bold"
        android:text="Detalles cita"
        android:textColor="#BE3838"
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="@+id/back_detalle_cita"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/back_detalle_cita"
        app:layout_constraintTop_toBottomOf="@+id/back_detalle_cita" />

    <ImageButton
        android:id="@+id/back_detalle_cita"
        android:layout_width="31dp"
        android:layout_height="26dp"
        android:layout_marginTop="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.042"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/backveterinaria" />

    <TextView
        android:id="@+id/textView35"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/comfortaa_bold"
        android:text="Motivo de atención"
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="@+id/textView34"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/textView34"
        app:layout_constraintTop_toBottomOf="@+id/textView34" />

    <TextView
        android:id="@+id/textView36"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:fontFamily="@font/comfortaa_bold"
        android:text="Diagnóstico"
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="@+id/edt_motivo_atencion_cita"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/edt_motivo_atencion_cita"
        app:layout_constraintTop_toBottomOf="@+id/edt_motivo_atencion_cita" />

    <TextView
        android:id="@+id/textView37"
        android:layout_width="110dp"
        android:layout_height="35dp"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/comfortaa_bold"
        android:text="Vacunas"
        android:textColor="#BE3838"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="@+id/edt_diagnostico_cita"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/edt_diagnostico_cita"
        app:layout_constraintTop_toBottomOf="@+id/edt_diagnostico_cita" />

    <TextView
        android:id="@+id/textView38"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:fontFamily="@font/comfortaa_bold"
        android:text="Dósis suministrada (ml)"
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="@+id/edt_vacuna_cita"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/edt_vacuna_cita"
        app:layout_constraintTop_toBottomOf="@+id/edt_vacuna_cita" />

    <TextView
        android:id="@+id/textView39"
        android:layout_width="132dp"
        android:layout_height="27dp"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/comfortaa_bold"
        android:text="Tipo de vacuna"
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="@+id/textView37"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/textView37"
        app:layout_constraintTop_toBottomOf="@+id/textView37" />

    <EditText
        android:id="@+id/edt_motivo_atencion_cita"
        android:layout_width="374dp"
        android:layout_height="37dp"
        android:backgroundTint="#BE3838"
        android:ems="10"
        android:inputType="textPersonName"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/textView35"
        app:layout_constraintTop_toBottomOf="@+id/textView35" />

    <EditText
        android:id="@+id/edt_diagnostico_cita"
        android:layout_width="376dp"
        android:layout_height="72dp"
        android:backgroundTint="#BE3838"
        android:ems="10"
        android:inputType="textPersonName"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/textView36"
        app:layout_constraintTop_toBottomOf="@+id/textView36" />

    <EditText
        android:id="@+id/edt_vacuna_cita"
        android:layout_width="374dp"
        android:layout_height="39dp"
        android:layout_marginTop="2dp"
        android:backgroundTint="#BE3838"
        android:ems="10"
        android:hint="Ingrese la vacuna"
        android:inputType="textPersonName"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/textView39"
        app:layout_constraintTop_toBottomOf="@+id/textView39" />

    <EditText
        android:id="@+id/edt_dosis_sumin_cita"
        android:layout_width="373dp"
        android:layout_height="38dp"
        android:backgroundTint="#BE3838"
        android:ems="10"
        android:hint="Ingrese la dósis suministrada"
        android:inputType="textPersonName"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/textView38"
        app:layout_constraintTop_toBottomOf="@+id/textView38" />

    <TextView
        android:id="@+id/textView40"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/comfortaa_bold"
        android:text="Fecha próxima cita"
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="@+id/edt_dosis_sumin_cita"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/edt_dosis_sumin_cita"
        app:layout_constraintTop_toBottomOf="@+id/edt_dosis_sumin_cita" />


    <EditText
        android:id="@+id/edt_fecha_cita"
        android:layout_width="376dp"
        android:layout_height="39dp"
        android:backgroundTint="#BE3838"
        android:clickable="true"
        android:focusable="false"
        android:hint="Fecha"
        android:inputType="date"
        android:maxLines="1"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/textView40"
        app:layout_constraintTop_toBottomOf="@+id/textView40" />

    <Button
        android:id="@+id/btn_guardar_detalle_cita"
        android:layout_width="379dp"
        android:layout_height="65dp"
        android:layout_marginTop="8dp"
        android:backgroundTint="#EF9292"
        android:text="guardar"
        android:textColor="#FFFFFF"
        app:layout_constraintEnd_toEndOf="@+id/edt_fecha_cita"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/edt_fecha_cita"
        app:layout_constraintTop_toBottomOf="@+id/edt_fecha_cita" />


</androidx.constraintlayout.widget.ConstraintLayout>