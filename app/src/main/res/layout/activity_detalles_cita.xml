<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".DetallesCitaActivity">

    <TextView
        android:id="@+id/textView34"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="Detalles cita"
        app:layout_constraintEnd_toEndOf="@+id/back_detalle_cita"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/back_detalle_cita"
        app:layout_constraintTop_toBottomOf="@+id/back_detalle_cita" />

    <ImageButton
        android:id="@+id/back_detalle_cita"
        android:layout_width="31dp"
        android:layout_height="26dp"
        android:layout_marginTop="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.042"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/backveterinaria" />

    <TextView
        android:id="@+id/textView35"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="Motivo de atención"
        app:layout_constraintEnd_toEndOf="@+id/textView34"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/textView34"
        app:layout_constraintTop_toBottomOf="@+id/textView34" />

    <TextView
        android:id="@+id/textView36"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="Diagnóstico"
        app:layout_constraintEnd_toEndOf="@+id/edt_motivo_atencion_cita"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/edt_motivo_atencion_cita"
        app:layout_constraintTop_toBottomOf="@+id/edt_motivo_atencion_cita" />

    <TextView
        android:id="@+id/textView37"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="Vacunas"
        app:layout_constraintEnd_toEndOf="@+id/edt_diagnostico_cita"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/edt_diagnostico_cita"
        app:layout_constraintTop_toBottomOf="@+id/edt_diagnostico_cita" />

    <TextView
        android:id="@+id/textView38"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="Dósis suministrada (mg)"
        app:layout_constraintEnd_toEndOf="@+id/edt_vacuna_cita"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/edt_vacuna_cita"
        app:layout_constraintTop_toBottomOf="@+id/edt_vacuna_cita" />

    <TextView
        android:id="@+id/textView39"
        android:layout_width="98dp"
        android:layout_height="29dp"
        android:layout_marginTop="8dp"
        android:text="Tipo de vacuna"
        app:layout_constraintEnd_toEndOf="@+id/textView37"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/textView37"
        app:layout_constraintTop_toBottomOf="@+id/textView37" />

    <EditText
        android:id="@+id/edt_motivo_atencion_cita"
        android:layout_width="360dp"
        android:layout_height="40dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="@+id/textView35"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/textView35"
        app:layout_constraintTop_toBottomOf="@+id/textView35" />

    <EditText
        android:id="@+id/edt_diagnostico_cita"
        android:layout_width="359dp"
        android:layout_height="78dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="@+id/textView36"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/textView36"
        app:layout_constraintTop_toBottomOf="@+id/textView36" />

    <EditText
        android:id="@+id/edt_vacuna_cita"
        android:layout_width="359dp"
        android:layout_height="46dp"
        android:layout_marginTop="2dp"
        android:ems="10"
        android:hint="Ingrese la vacuna"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="@+id/textView39"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/textView39"
        app:layout_constraintTop_toBottomOf="@+id/textView39" />

    <EditText
        android:id="@+id/edt_dosis_sumin_cita"
        android:layout_width="357dp"
        android:layout_height="45dp"
        android:ems="10"
        android:hint="Ingrese la dósis suministrada"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="@+id/textView38"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/textView38"
        app:layout_constraintTop_toBottomOf="@+id/textView38" />

    <TextView
        android:id="@+id/textView40"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="Fecha próxima cita"
        app:layout_constraintEnd_toEndOf="@+id/edt_dosis_sumin_cita"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/edt_dosis_sumin_cita"
        app:layout_constraintTop_toBottomOf="@+id/edt_dosis_sumin_cita" />


    <EditText
        android:id="@+id/edt_fecha_cita"
        android:layout_width="357dp"
        android:layout_height="40dp"
        android:clickable="true"
        android:focusable="false"
        android:hint="Fecha"
        android:inputType="date"
        android:maxLines="1"
        app:layout_constraintEnd_toEndOf="@+id/textView40"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/textView40"
        app:layout_constraintTop_toBottomOf="@+id/textView40" />

    <Button
        android:id="@+id/btn_guardar_detalle_cita"
        android:layout_width="370dp"
        android:layout_height="44dp"
        android:layout_marginTop="8dp"
        android:text="guardar"
        app:layout_constraintEnd_toEndOf="@+id/edt_fecha_cita"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/edt_fecha_cita"
        app:layout_constraintTop_toBottomOf="@+id/edt_fecha_cita" />


</androidx.constraintlayout.widget.ConstraintLayout>