<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MenuDuenioMascotaActivity">

    <TextView
        android:id="@+id/textView55"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="2dp"
        android:text="Citas solicitadas"
        android:textColor="@android:color/black"
        app:layout_constraintEnd_toEndOf="@+id/edt_fecha_cita_duenio"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/edt_fecha_cita_duenio"
        app:layout_constraintTop_toBottomOf="@+id/edt_fecha_cita_duenio" />

    <TextView
        android:id="@+id/textView49"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:text="Mascotas"
        android:textColor="#BE3838"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.093"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView54"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="Citas"
        android:textColor="#BE3838"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="@+id/btn_mascota_nueva_menu_duenio"
        app:layout_constraintHorizontal_bias="0.021"
        app:layout_constraintStart_toStartOf="@+id/btn_mascota_nueva_menu_duenio"
        app:layout_constraintTop_toBottomOf="@+id/btn_mascota_nueva_menu_duenio" />

    <TextView
        android:id="@+id/textView52"
        android:layout_width="58dp"
        android:layout_height="16dp"
        android:layout_marginStart="68dp"
        android:text="Raza"
        android:textColor="@android:color/black"
        app:layout_constraintBottom_toBottomOf="@+id/textView53"
        app:layout_constraintStart_toEndOf="@+id/textView53"
        app:layout_constraintTop_toTopOf="@+id/textView53"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView50"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="Nombre"
        android:textColor="@android:color/black"
        app:layout_constraintEnd_toEndOf="@+id/textView51"
        app:layout_constraintHorizontal_bias="0.11"
        app:layout_constraintStart_toStartOf="@+id/textView51"
        app:layout_constraintTop_toBottomOf="@+id/textView51" />

    <TextView
        android:id="@+id/textView53"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="64dp"
        android:text="Especie"
        android:textColor="@android:color/black"
        app:layout_constraintBottom_toBottomOf="@+id/textView50"
        app:layout_constraintStart_toEndOf="@+id/textView50"
        app:layout_constraintTop_toTopOf="@+id/textView50"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/textView51"
        android:layout_width="151dp"
        android:layout_height="20dp"
        android:layout_marginTop="12dp"
        android:text="Mascotas Disponibles"
        android:textColor="@android:color/black"
        app:layout_constraintEnd_toEndOf="@+id/edt_nombre_mascota_duenio"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/edt_nombre_mascota_duenio"
        app:layout_constraintTop_toBottomOf="@+id/edt_nombre_mascota_duenio" />

    <EditText
        android:id="@+id/edt_nombre_mascota_duenio"
        android:layout_width="321dp"
        android:layout_height="42dp"
        android:layout_marginTop="12dp"
        android:ems="10"
        android:hint="Ingrese el nombre de la mascota"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="@+id/textView49"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/textView49"
        app:layout_constraintTop_toBottomOf="@+id/textView49" />

    <EditText
        android:id="@+id/edt_fecha_cita_duenio"
        android:layout_width="336dp"
        android:layout_height="41dp"
        android:layout_marginTop="2dp"
        android:clickable="true"
        android:ems="10"
        android:focusable="false"
        android:hint="Seleccione la fecha de la cita"
        android:inputType="date"
        android:maxLines="1"
        app:layout_constraintEnd_toEndOf="@+id/textView54"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/textView54"
        app:layout_constraintTop_toBottomOf="@+id/textView54" />

    <ListView
        android:id="@+id/lv_mascotas"
        android:layout_width="346dp"
        android:layout_height="144dp"
        app:layout_constraintEnd_toEndOf="@+id/textView52"
        app:layout_constraintHorizontal_bias="0.25"
        app:layout_constraintStart_toStartOf="@+id/textView50"
        app:layout_constraintTop_toBottomOf="@+id/textView53" />

    <Button
        android:id="@+id/btn_mascota_nueva_menu_duenio"
        android:layout_width="343dp"
        android:layout_height="39dp"
        android:layout_marginTop="2dp"
        android:backgroundTint="#CF2626"
        android:text="Nueva mascota"
        android:textColor="@android:color/background_light"
        app:layout_constraintEnd_toEndOf="@+id/lv_mascotas"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/lv_mascotas"
        app:layout_constraintTop_toBottomOf="@+id/lv_mascotas" />

    <ListView
        android:id="@+id/lv_citas"
        android:layout_width="347dp"
        android:layout_height="87dp"
        app:layout_constraintEnd_toEndOf="@+id/textView58"
        app:layout_constraintHorizontal_bias="0.068"
        app:layout_constraintStart_toStartOf="@+id/textView56"
        app:layout_constraintTop_toBottomOf="@+id/textView57" />

    <TextView
        android:id="@+id/textView56"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="2dp"
        android:text="Nombre"
        android:textColor="@android:color/black"
        app:layout_constraintEnd_toEndOf="@+id/textView55"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/textView55"
        app:layout_constraintTop_toBottomOf="@+id/textView55" />

    <TextView
        android:id="@+id/textView57"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="36dp"
        android:text="Hora"
        android:textColor="@android:color/black"
        app:layout_constraintBottom_toBottomOf="@+id/textView56"
        app:layout_constraintStart_toEndOf="@+id/textView56"
        app:layout_constraintTop_toTopOf="@+id/textView56"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView58"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="60dp"
        android:text="Fecha"
        android:textColor="@android:color/black"
        app:layout_constraintBottom_toBottomOf="@+id/textView57"
        app:layout_constraintStart_toEndOf="@+id/textView57"
        app:layout_constraintTop_toTopOf="@+id/textView57"
        app:layout_constraintVertical_bias="0.0" />

    <Spinner
        android:id="@+id/sp_duenio_mascota"
        android:layout_width="32dp"
        android:layout_height="20dp"
        android:layout_marginTop="12dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.957"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_menu_duenio_mascota"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:clickable="true"
        android:text="Carlos Rodas"
        android:textColor="@android:color/black"
        app:layout_constraintBottom_toBottomOf="@+id/sp_duenio_mascota"
        app:layout_constraintEnd_toStartOf="@+id/sp_duenio_mascota"
        app:layout_constraintTop_toTopOf="@+id/sp_duenio_mascota"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/textView107"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@carlosrodas"
        app:layout_constraintEnd_toEndOf="@+id/tv_menu_duenio_mascota"
        app:layout_constraintStart_toStartOf="@+id/tv_menu_duenio_mascota"
        app:layout_constraintTop_toBottomOf="@+id/tv_menu_duenio_mascota" />

    <TextView
        android:id="@+id/textView108"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="64dp"
        android:text="Estado"
        android:textColor="@android:color/black"
        app:layout_constraintBottom_toBottomOf="@+id/textView58"
        app:layout_constraintStart_toEndOf="@+id/textView58"
        app:layout_constraintTop_toTopOf="@+id/textView58"
        app:layout_constraintVertical_bias="1.0" />

</androidx.constraintlayout.widget.ConstraintLayout>