<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".RegistroVeterinarioActivity">

    <ImageButton
        android:id="@+id/btn_back_registro_veterinario"
        android:layout_width="28dp"
        android:layout_height="23dp"
        android:layout_marginTop="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.041"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/backveterinaria" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="149dp"
        android:layout_height="25dp"
        android:layout_marginTop="32dp"
        android:text="Registro veterinario"
        app:layout_constraintEnd_toEndOf="@+id/btn_back_registro_veterinario"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/btn_back_registro_veterinario"
        app:layout_constraintTop_toBottomOf="@+id/btn_back_registro_veterinario" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="28dp"
        android:text="Cédula de identidad"
        app:layout_constraintEnd_toEndOf="@+id/textView8"
        app:layout_constraintHorizontal_bias="0.48"
        app:layout_constraintStart_toStartOf="@+id/textView8"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />

    <EditText
        android:id="@+id/edt_nombre_registro_veterinario"
        android:layout_width="329dp"
        android:layout_height="46dp"
        android:ems="10"
        android:hint="Ingrese su nombre"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="@+id/textView9"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/textView9"
        app:layout_constraintTop_toBottomOf="@+id/textView9" />

    <EditText
        android:id="@+id/edt_apellido_registro_veterinario"
        android:layout_width="328dp"
        android:layout_height="46dp"
        android:ems="10"
        android:hint="Ingrese su apellido"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="@+id/textView10"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/textView10"
        app:layout_constraintTop_toBottomOf="@+id/textView10" />

    <EditText
        android:id="@+id/edt_celular_registro_veterinario"
        android:layout_width="328dp"
        android:layout_height="41dp"
        android:ems="10"
        android:hint="Ingrese su celular"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="@+id/textView11"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/textView11"
        app:layout_constraintTop_toBottomOf="@+id/textView11" />

    <EditText
        android:id="@+id/edt_experi_registro_veterinario"
        android:layout_width="328dp"
        android:layout_height="48dp"
        android:ems="10"
        android:hint="Ingrese sus años de experiencia laboral"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="@+id/textView12"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/textView12"
        app:layout_constraintTop_toBottomOf="@+id/textView12" />

    <EditText
        android:id="@+id/edt_usuario_registro_veterinario"
        android:layout_width="325dp"
        android:layout_height="41dp"
        android:ems="10"
        android:hint="Ingrese su usuario"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="@+id/textView13"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/textView13"
        app:layout_constraintTop_toBottomOf="@+id/textView13" />

    <EditText
        android:id="@+id/edt_contraseni_registro_veterinario"
        android:layout_width="326dp"
        android:layout_height="50dp"
        android:ems="10"
        android:hint="Ingrese su contraseña"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="@+id/textView14"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/textView14"
        app:layout_constraintTop_toBottomOf="@+id/textView14" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Apellido"
        app:layout_constraintEnd_toEndOf="@+id/edt_nombre_registro_veterinario"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/edt_nombre_registro_veterinario"
        app:layout_constraintTop_toBottomOf="@+id/edt_nombre_registro_veterinario" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:text="Celular"
        app:layout_constraintEnd_toEndOf="@+id/edt_apellido_registro_veterinario"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/edt_apellido_registro_veterinario"
        app:layout_constraintTop_toBottomOf="@+id/edt_apellido_registro_veterinario" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Años de experiencia laboral"
        app:layout_constraintEnd_toEndOf="@+id/edt_celular_registro_veterinario"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/edt_celular_registro_veterinario"
        app:layout_constraintTop_toBottomOf="@+id/edt_celular_registro_veterinario" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Usuario"
        app:layout_constraintEnd_toEndOf="@+id/edt_experi_registro_veterinario"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/edt_experi_registro_veterinario"
        app:layout_constraintTop_toBottomOf="@+id/edt_experi_registro_veterinario" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Contraseña"
        app:layout_constraintEnd_toEndOf="@+id/edt_usuario_registro_veterinario"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/edt_usuario_registro_veterinario"
        app:layout_constraintTop_toBottomOf="@+id/edt_usuario_registro_veterinario" />

    <Button
        android:id="@+id/btn_registrar_veterinario"
        android:layout_width="357dp"
        android:layout_height="56dp"
        android:layout_marginTop="12dp"
        android:text="REGISTRARSE"
        app:layout_constraintEnd_toEndOf="@+id/edt_contraseni_registro_veterinario"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/edt_contraseni_registro_veterinario"
        app:layout_constraintTop_toBottomOf="@+id/edt_contraseni_registro_veterinario" />
</androidx.constraintlayout.widget.ConstraintLayout>